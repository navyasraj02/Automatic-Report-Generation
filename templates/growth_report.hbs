      }
      .legend-color {
        width: 10px;
        height: 7px;
        border-radius: 2px;
      }
      .legend-expected {
        background: linear-gradient(180deg, #ffd700 0%, #ffa500 100%);
      }
      .legend-achieved {
        background: linear-gradient(180deg, #00ced1 0%, #008b8b 100%);
      }
      .progress-slide {
        padding: 50px 70px;
      }
      .progress-content {
        display: grid;
        grid-template-columns: 400px 1fr;
        gap: 40px;
        z-index: 10;
        position: relative;
      }
      .progress-description {
        background: rgba(26, 26, 26, 0.95);
        border-radius: 20px;
        padding: 35px;
        border: 2px solid #333;
        display: flex;
        flex-direction: column;
        justify-content: center;
      }
      .progress-chart-container {
        background: rgba(26, 26, 26, 0.95);
        border-radius: 20px;
        padding: 40px 30px;
        border: 2px solid #333;
        display: flex;
        align-items: center;
        justify-content: center;
      }
      .chart-with-axis {
        display: flex;
        gap: 0;
        width: 100%;
        height: 100%;
      }

      /* Shared bar container styles */
      .stacked-bars,
      .multi-bars,
      .activity-bars {
        display: flex;
        align-items: flex-end;
        justify-content: space-around;
        height: 280px;
        flex: 1;
        padding: 60px 15px 0;
      }

      /* Shared bar group styles */
      .stacked-bar-group,
      .multi-bar-group,
      .activity-bar-group {
        display: flex;
        flex-direction: column;
        align-items: center;
        flex: 1 1 0;
        min-width: 0;
      }

      /* Individual bar styles */
      .stacked-bar {
        width: 100%;
        max-width: 100px;
        background: linear-gradient(
          to bottom,
          rgba(200, 16, 46, 1) 0%,
          rgba(200, 16, 46, 0.8) 100%
        );
        border-radius: 12px 12px 0 0;
        position: relative;
        box-shadow: 0 4px 20px rgba(200, 16, 46, 0.3);
        min-height: 5px;
      }

      .multi-bar {
        width: 100%;
        max-width: 80px;
        background: linear-gradient(
          to bottom,
          rgba(200, 16, 46, 1) 0%,
          rgba(200, 16, 46, 0.8) 100%
        );
        border-radius: 12px 12px 0 0;
        position: relative;
        box-shadow: 0 4px 20px rgba(200, 16, 46, 0.3);
        min-height: 5px;
      }

      .activity-bar {
        width: 100%;
        max-width: 65px;
        background: linear-gradient(
          to bottom,
          rgba(200, 16, 46, 1) 0%,
          rgba(200, 16, 46, 0.8) 100%
        );
        border-radius: 10px 10px 0 0;
        position: relative;
        box-shadow: 0 4px 20px rgba(200, 16, 46, 0.3);
        min-height: 5px;
      }

      /* Bar label styles */
      .stacked-bar-label {
        position: absolute;
        top: -45px;
        left: 50%;
        transform: translateX(-50%);
        font-family: "Bebas Neue", Arial, sans-serif;
        font-size: 38px;
        font-weight: 400;
        color: #c8102e;
        white-space: nowrap;
        text-shadow: 0 2px 10px rgba(0, 0, 0, 0.3);
      }

      .multi-bar-label {
        position: absolute;
        top: -45px;
        left: 50%;
        transform: translateX(-50%);
        font-family: "Bebas Neue", Arial, sans-serif;
        font-size: 34px;
        font-weight: 400;
        color: #c8102e;
        white-space: nowrap;
