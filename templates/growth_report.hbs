  <body>
    <!-- SLIDE 1: COVER -->
    <div class="slide cover-slide">
      <div class="circle circle-top-left"></div>
      <div class="circle circle-top-right"></div>
      <div class="circle circle-small"></div>
      <div class="circle circle-middle-right"></div>
      <div class="circle circle-bottom-right"></div>
      <div class="circle circle-middle-left"></div>

      <div class="logo-container">
        <img src="{{{logoBase64}}}" alt="HPE DSI Logo" class="logo" />
      </div>

      <div class="badge">Growth Report</div>

      <h1 class="title">{{experience}}</h1>

      <p class="subtitle">Fall 2024</p>
      <p class="instructor">Instructor: Dr. Ioannis Konstantinidis</p>
    </div>

    <!-- SLIDE 2: STUDENT PARTICIPATION -->
    <div class="slide participation-slide">
      <div class="circle circle-top-left"></div>
      <div class="circle circle-middle-left"></div>
      <div class="circle circle-bottom-right"></div>

      <div class="slide-header">
        <h2 class="slide-title">Student Participation</h2>
      </div>

      <div class="content-grid">
        <div class="chart-container">
          <div class="dual-bar-chart-with-axis">
            <div class="y-axis">
              <span>100%</span>
              <span>75%</span>
              <span>50%</span>
              <span>25%</span>
              <span>0%</span>
            </div>

            <div class="chart-area">
              <div class="dual-bar-wrapper">
                <div>
                  <div
                    class="bar"
                    style="height: {{multiply goalSettingPercentage 3}}px;"
                  >
                    <div class="bar-label">{{goalSettingPercentage}}%</div>
                  </div>
                </div>
                <div>
                  <div
                    class="bar"
                    style="height: {{multiply exitFormPercentage 3}}px;"
                  >
                    <div class="bar-label">{{exitFormPercentage}}%</div>
                  </div>
                </div>
              </div>
              <div class="chart-labels">
                <div class="bar-base">Goal-Setting<br />Form</div>
                <div class="bar-base">Exit<br />Form</div>
              </div>
            </div>
          </div>
        </div>

        <div class="stats-container">
          <div class="stat-card">
            <div class="stat-label">Goal-Setting Form</div>
            <div class="stat-value">{{goalSettingPercentage}}%</div>
            <div
              class="stat-description"
            >{{goalSettingCompleted}}/{{totalRegistered}}
              registered students completed the goal-setting form</div>
          </div>

          <div class="divider"></div>

          <div class="stat-card">
            <div class="stat-label">Exit Form</div>
            <div class="stat-value">{{exitFormPercentage}}%</div>
            <div
              class="stat-description"
            >{{exitFormCompleted}}/{{totalRegistered}}
              registered students completed the exit form</div>
          </div>
        </div>
      </div>
    </div>

    <!-- SLIDE 3: STUDENT GROWTH -->
    <div class="slide growth-slide">
      <div class="circle circle-top-right"></div>
      <div class="circle circle-bottom-left"></div>
      <div class="circle circle-middle-right"></div>

      <div class="slide-header">
        <h2 class="slide-title">Student Growth</h2>
      </div>

      <div class="growth-content">
        <div class="growth-description">
          <p class="growth-text">Students were presented with six different
            factors on the goal-setting form where they indicated how much
            growth they expect in that factor from
            {{experience}}. In the exit form at the end of the school year,
            students then indicated how much growth they actually experienced in
            each factor from
            {{experience}}.</p>

          <div class="growth-scale">
            <div class="growth-scale-title">The range of growth options were:</div>
            <div class="growth-scale-item">None</div>
            <div class="growth-scale-item">Little</div>
            <div class="growth-scale-item">Moderate</div>
            <div class="growth-scale-item">A lot</div>
          </div>
        </div>

        <div class="growth-charts">
          <div class="mini-chart">
            <div class="mini-chart-title">Growth in teamwork</div>
            <div class="mini-bars-wrapper">
              {{#each studentGrowth.teamwork}}
                <div class="mini-bar-group">
                  <div class="mini-bars">
                    <div
                      class="mini-bar-expected"
                      style="height: calc({{this.expected}}% * 0.7);"
                    ></div>
                    <div
                      class="mini-bar-achieved"
                      style="height: calc({{this.achieved}}% * 0.7);"
                    ></div>
                  </div>
                  <div class="mini-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
            <div class="mini-chart-legend">
              <div class="legend-item"><div
                  class="legend-color legend-expected"
                ></div>
                Expected</div>
              <div class="legend-item"><div
                  class="legend-color legend-achieved"
                ></div>
                Achieved</div>
            </div>
          </div>

          <div class="mini-chart">
            <div class="mini-chart-title">Growth in professional responsibility</div>
            <div class="mini-bars-wrapper">
              {{#each studentGrowth.professionalResponsibility}}
                <div class="mini-bar-group">
                  <div class="mini-bars">
                    <div
                      class="mini-bar-expected"
                      style="height: calc({{this.expected}}% * 0.7);"
                    ></div>
                    <div
                      class="mini-bar-achieved"
                      style="height: calc({{this.achieved}}% * 0.7);"
                    ></div>
                  </div>
                  <div class="mini-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
            <div class="mini-chart-legend">
              <div class="legend-item"><div
                  class="legend-color legend-expected"
                ></div>
                Expected</div>
              <div class="legend-item"><div
                  class="legend-color legend-achieved"
                ></div>
                Achieved</div>
            </div>
          </div>

          <div class="mini-chart">
            <div class="mini-chart-title">Growth in effective communication</div>
            <div class="mini-bars-wrapper">
              {{#each studentGrowth.effectiveCommunication}}
                <div class="mini-bar-group">
                  <div class="mini-bars">
                    <div
                      class="mini-bar-expected"
                      style="height: calc({{this.expected}}% * 0.7);"
                    ></div>
                    <div
                      class="mini-bar-achieved"
                      style="height: calc({{this.achieved}}% * 0.7);"
                    ></div>
                  </div>
                  <div class="mini-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
            <div class="mini-chart-legend">
              <div class="legend-item"><div
                  class="legend-color legend-expected"
                ></div>
                Expected</div>
              <div class="legend-item"><div
                  class="legend-color legend-achieved"
                ></div>
                Achieved</div>
            </div>
          </div>

          <div class="mini-chart">
            <div class="mini-chart-title">Growth in problem solving</div>
            <div class="mini-bars-wrapper">
              {{#each studentGrowth.problemSolving}}
                <div class="mini-bar-group">
                  <div class="mini-bars">
                    <div
                      class="mini-bar-expected"
                      style="height: calc({{this.expected}}% * 0.7);"
                    ></div>
                    <div
                      class="mini-bar-achieved"
                      style="height: calc({{this.achieved}}% * 0.7);"
                    ></div>
                  </div>
                  <div class="mini-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
            <div class="mini-chart-legend">
              <div class="legend-item"><div
                  class="legend-color legend-expected"
                ></div>
                Expected</div>
              <div class="legend-item"><div
                  class="legend-color legend-achieved"
                ></div>
                Achieved</div>
            </div>
          </div>

          <div class="mini-chart">
            <div class="mini-chart-title">Growth in cultural humility</div>
            <div class="mini-bars-wrapper">
              {{#each studentGrowth.culturalHumility}}
                <div class="mini-bar-group">
                  <div class="mini-bars">
                    <div
                      class="mini-bar-expected"
                      style="height: calc({{this.expected}}% * 0.7);"
                    ></div>
                    <div
                      class="mini-bar-achieved"
                      style="height: calc({{this.achieved}}% * 0.7);"
                    ></div>
                  </div>
                  <div class="mini-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
            <div class="mini-chart-legend">
              <div class="legend-item"><div
                  class="legend-color legend-expected"
                ></div>
                Expected</div>
              <div class="legend-item"><div
                  class="legend-color legend-achieved"
                ></div>
                Achieved</div>
            </div>
          </div>

          <div class="mini-chart">
            <div class="mini-chart-title">Growth in ethical decision making</div>
            <div class="mini-bars-wrapper">
              {{#each studentGrowth.ethicalDecisionMaking}}
                <div class="mini-bar-group">
                  <div class="mini-bars">
                    <div
                      class="mini-bar-expected"
                      style="height: calc({{this.expected}}% * 0.7);"
                    ></div>
                    <div
                      class="mini-bar-achieved"
                      style="height: calc({{this.achieved}}% * 0.7);"
                    ></div>
                  </div>
                  <div class="mini-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
            <div class="mini-chart-legend">
              <div class="legend-item"><div
                  class="legend-color legend-expected"
                ></div>
                Expected</div>
              <div class="legend-item"><div
                  class="legend-color legend-achieved"
                ></div>
                Achieved</div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- SLIDE 4: PROGRESS TOWARDS GOALS -->
    <div class="slide progress-slide">
      <div class="circle circle-top-left"></div>
      <div class="circle circle-middle-right"></div>
      <div class="circle circle-bottom-right"></div>

      <div class="slide-header">
        <h2 class="slide-title">Progress Towards Goals</h2>
      </div>

      <div class="progress-content">
        <div class="progress-description">
          <p class="growth-text">Students were asked to assess how much progress
            they made in regard to achieving their goals set at the beginning of
            the school year.</p>

          <p class="growth-text">On average, students felt they made some to
            lots of progress towards their goals set in
            {{experience}}.
            {{totalGoals}}
            goals were cumulatively set across all students.</p>

          <div class="growth-scale">
            <div class="growth-scale-title">The scale is as follows:</div>
            <div class="growth-scale-item">I did not make progress towards this
              goal</div>
            <div class="growth-scale-item">I made little progress towards this
              goal</div>
            <div class="growth-scale-item">I made some progress towards this
              goal</div>
            <div class="growth-scale-item">I made lots of progress towards this
              goal</div>
          </div>
        </div>

        <div class="progress-chart-container">
          <div class="chart-with-axis">
            <div class="stacked-bars">
              {{#each progressTowardsGoals}}
                <div class="stacked-bar-group">
                  {{#if (gt this.percentage 0)}}
                    <div
                      class="stacked-bar"
                      style="height: {{multiply this.percentage 2.8}}px;"
                    >
                      <div class="stacked-bar-label">{{this.percentage}}%</div>
                    </div>
                  {{else}}
                    <div class="stacked-bar" style="height: 5px; opacity: 0.3;">
                      <div
                        class="stacked-bar-label"
                        style="display: none;"
                      >{{this.percentage}}%</div>
                    </div>
                  {{/if}}
                  <div class="stacked-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- SLIDE 5: EXPERIENCE CONNECTION -->
    <div class="slide progress-slide">
      <div class="circle circle-top-right"></div>
      <div class="circle circle-bottom-left"></div>
      <div class="circle circle-middle-left"></div>

      <div class="slide-header">
        <h2 class="slide-title">Experience Connection</h2>
      </div>

      <div class="progress-content">
        <div class="progress-description">
          <p class="growth-text">Students were asked to individually assess each
            goal they set at the beginning of the school year, and quantify as
            to how useful their experience in
            {{experience}}
            was in conjunction with achieving that goal.</p>

          <p class="growth-text">{{totalGoals}}
            goals were cumulatively set across all students.</p>

          <div class="growth-scale">
            <div class="growth-scale-title">The scale is as follows:</div>
            <div class="growth-scale-item">The progress I made towards this goal
              was not due to this experience</div>
            <div class="growth-scale-item">The progress I made towards this goal
              was partly due to this experience</div>
            <div class="growth-scale-item">The progress I made towards this goal
              was largely due to this experience</div>
          </div>
        </div>

        <div class="progress-chart-container">
          <div class="chart-with-axis">
            <div class="stacked-bars">
              {{#each experienceConnection}}
                <div class="stacked-bar-group">
                  {{#if (gt this.percentage 0)}}
                    <div
                      class="stacked-bar"
                      style="height: {{multiply this.percentage 2.8}}px;"
                    >
                      <div class="stacked-bar-label">{{this.percentage}}%</div>
                    </div>
                  {{else}}
                    <div class="stacked-bar" style="height: 5px; opacity: 0.3;">
                      <div
                        class="stacked-bar-label"
                        style="display: none;"
                      >{{this.percentage}}%</div>
                    </div>
                  {{/if}}
                  <div class="stacked-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- SLIDE 6: NET PROMOTER -->
    <div class="slide progress-slide">
      <div class="circle circle-top-left"></div>
      <div class="circle circle-middle-right"></div>
      <div class="circle circle-bottom-right"></div>

      <div class="slide-header">
        <h2 class="slide-title">Net Promoter</h2>
      </div>

      <div class="progress-content">
        <div class="progress-description">
          <p class="growth-text">After taking this class:</p>

          <div class="nps-list">
            {{#each netPromoterDescriptions}}
              <div class="nps-list-item">
                <span class="nps-label">{{this.label}}:</span>{{this.text}}
              </div>
            {{/each}}
          </div>
        </div>

        <div class="progress-chart-container">
          <div class="chart-with-axis">
            <div class="multi-bars">
              {{#each netPromoterScores}}
                <div class="multi-bar-group">
                  {{#if (gt this.percentage 0)}}
                    <div
                      class="multi-bar"
                      style="height: {{multiply this.percentage 2.8}}px;"
                    >
                      <div class="multi-bar-label">{{this.percentage}}%</div>
                    </div>
                  {{else}}
                    <div class="multi-bar" style="height: 5px; opacity: 0.3;">
                      <div
                        class="multi-bar-label"
                        style="display: none;"
                      >{{this.percentage}}%</div>
                    </div>
                  {{/if}}
                  <div class="multi-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- SLIDE 7: ACTIVITIES -->
    <div class="slide progress-slide">
      <div class="circle circle-top-right"></div>
      <div class="circle circle-bottom-left"></div>
      <div class="circle circle-middle-left"></div>

      <div class="slide-header">
        <h2 class="slide-title">Activities</h2>
      </div>

      <div class="progress-content">
        <div class="progress-description">
          <p class="growth-text">Students were presented with a list of
            activities to choose which from
            {{experience}}
            were most helpful in contributing to their achieved goals. Below are
            the activities ranked by percentage of total goals set among
            students ({{totalGoals}}) attributing each activity to helping
            achieve each goal:</p>

          <div class="activity-list">
            {{#each activitiesDescriptions}}
              <div class="activity-list-item">
                <span class="activity-label">{{this.label}}:</span>{{this.text}}
              </div>
            {{/each}}
          </div>
        </div>

        <div class="progress-chart-container">
          <div class="chart-with-axis">
            <div class="activity-bars">
              {{#each activitiesScores}}
                <div class="activity-bar-group">
                  {{#if (gt this.percentage 0)}}
                    <div
                      class="activity-bar"
                      style="height: {{multiply this.percentage 2.8}}px;"
                    >
                      <div class="activity-bar-label">{{this.percentage}}%</div>
                    </div>
                  {{else}}
                    <div
                      class="activity-bar"
                      style="height: 5px; opacity: 0.3;"
                    >
                      <div
                        class="activity-bar-label"
                        style="display: none;"
                      >{{this.percentage}}%</div>
                    </div>
                  {{/if}}
                  <div class="activity-bar-category">{{this.category}}</div>
                </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- SLIDE 8: TESTIMONIALS HEADING -->
    <div class="slide testimonials-heading-slide">
      <div class="circle circle-top-left"></div>
      <div class="circle circle-bottom-right"></div>
      <div class="circle circle-middle-right"></div>

      <h1 class="testimonials-title">Student<br />Testimonials</h1>
    </div>

    <!-- SLIDE 9: BIGGEST LESSONS LEARNED -->
    <div class="slide testimonials-slide">
      <div class="circle circle-top-right"></div>
      <div class="circle circle-middle-left"></div>
      <div class="circle circle-bottom-left"></div>

      <div class="slide-header">
        <h2 class="slide-title">Biggest Lessons Learned</h2>
      </div>

      <div class="testimonials-container">
        {{#if biggestLessons}}
          {{#each biggestLessons}}
            <div class="testimonial-card">
              <p class="testimonial-text">"{{this}}"</p>
            </div>
          {{/each}}
        {{else}}
          <div class="testimonial-card">
            <p class="testimonial-empty">No responses available</p>
          </div>
        {{/if}}
      </div>
    </div>

    <!-- SLIDE 10: EXPERIENCE CONTRIBUTIONS -->
    <div class="slide testimonials-slide">
      <div class="circle circle-top-left"></div>
      <div class="circle circle-middle-right"></div>
      <div class="circle circle-bottom-right"></div>

      <div class="slide-header">
        <h2 class="slide-title">Experience Contributions</h2>
      </div>

      <div class="testimonials-container">
        {{#if experienceContributions}}
          {{#each experienceContributions}}
            <div class="testimonial-card">
              <p class="testimonial-text">"{{this}}"</p>
            </div>
          {{/each}}
        {{else}}
          <div class="testimonial-card">
            <p class="testimonial-empty">No responses available</p>
          </div>
        {{/if}}
      </div>
    </div>

    <!-- SLIDE 11: ADDITIONAL COMMENTS -->
    <div class="slide testimonials-slide">
      <div class="circle circle-top-right"></div>
      <div class="circle circle-bottom-left"></div>
      <div class="circle circle-middle-left"></div>

      <div class="slide-header">
        <h2 class="slide-title">Additional Comments</h2>
      </div>

      <div class="testimonials-container">
        {{#if additionalComments}}
          {{#each additionalComments}}
            <div class="testimonial-card">
              <p class="testimonial-text">"{{this}}"</p>
            </div>
          {{/each}}
        {{else}}
          <div class="testimonial-card">
            <p class="testimonial-empty">No responses available</p>
          </div>
        {{/if}}
      </div>
    </div>
  </body>
</html>