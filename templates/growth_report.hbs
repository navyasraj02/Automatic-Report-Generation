      }
      .nps-list-item {
        font-size: 13px;
        color: #ccc;
        margin-bottom: 12px;
        line-height: 1.6;
      }
      .nps-label {
        font-weight: 700;
        color: #c8102e;
        margin-right: 5px;
      }
      .activity-list {
        margin-top: 20px;
      }
      .activity-list-item {
        font-size: 12px;
        color: #ccc;
        margin-bottom: 10px;
