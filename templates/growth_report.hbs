        word-break: break-word;
      }

      .multi-bar-category {
        font-size: 10px;
      }

      .activity-bar-category {
        font-size: 9px;
        margin-top: 8px;
      }

      .testimonials-heading-slide {
        padding: 60px 80px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
      }
      .testimonials-title {
        font-family: "Bebas Neue", Arial, sans-serif;
        font-size: 120px;
        font-weight: 400;
        color: #c8102e;
        text-align: center;
        text-transform: uppercase;
        letter-spacing: 3px;
        z-index: 10;
        position: relative;
      }
      .testimonials-slide {
        padding: 60px 80px;
      }
      .testimonials-container {
        z-index: 10;
        position: relative;
      }
      .testimonial-card {
        background: rgba(26, 26, 26, 0.95);
        border: 2px solid #333;
        border-radius: 20px;
        padding: 30px 40px;
        margin-bottom: 25px;
      }
      .testimonial-card:last-child {
        margin-bottom: 0;
      }
      .testimonial-text {
        font-size: 16px;
        color: #ccc;
        line-height: 1.8;
        font-weight: 400;
        font-style: italic;
      }
      .testimonial-empty {
        color: #666;
        text-align: center;
        padding: 20px;
      }
      .nps-list {
